   "source": [
    "s = str(input(\"Enter string \")).lower()\n",
    "\n",
    "def is_pangram(s):\n",
    "    b = set()\n",
    "    a = {\"a\",\"b\",\"c\",\"d\",\"e\",\"f\",\"g\",\"h\",\"i\",\"j\",\"k\",\"l\",\"m\",\"n\",\"o\",\"p\",\"q\",\"r\",\"s\",\"t\",\"u\",\"v\",\"w\",\"x\",\"y\",\"z\"}\n",
    "    for c in s:\n",
    "        if c != ' ':\n",
    "            b.add(c)\n",
    "    return a==b\n",
    "\n",
    "print(is_pangram(s))\n"
   ]
  }
